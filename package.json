{
	"name": "react-library-with-webpack",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"start": "webpack-dev-server --config ./webpack/dev.config.js --watch",
		"build": "webpack --config ./webpack/prod.config.js",
		"test": "jest --config ./test/jest.config.js --verbose",
		"test:watch": "jest --config ./test/jest.config.js --verbose --watchAll",
		"coverage": "jest --config ./test/jest.config.js --coverage --colors",
		"lint": "eslint ./src",
		"lint:fix": "eslint --fix ./src",
		"sass-lint": "sass-lint -vq src/**/*.scss",
		"lint-sonar": "eslint ./src -f json -o eslint-sonar.json",
		"sonar": "sonar-scanner.bat",
		"quality-check": "npm run lint && npm run sass-lint && npm run test && npm run coverage && npm run lint-sonar",
		"sonar-check": "npm run quality-check && npm run sonar",
		"prettier-format": "prettier --config .prettierrc src/**/*.js --write",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cvelu79/react-library-with-webpack.git"
	},
	"author": "cvelu79@gmail.com",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/cvelu79/react-library-with-webpack/issues"
	},
	"homepage": "https://github.com/cvelu79/react-library-with-webpack#readme",
	"devDependencies": {
